    "@graphql-yoga/nestjs": "^1.0.0",
    "@mdx-js/react": "^1.6.22",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/testing": "^9.2.0",
    "@prisma/client": "^4.5.0",
    "bcrypt": "^5.1.0",
