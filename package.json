  "devDependencies": {
    "@docusaurus/module-type-aliases": "2.1.0",
    "@nestjs/schematics": "^9.0.0",
    "@nrwl/cli": "15.8.3",
    "@nrwl/devkit": "15.8.3",
    "@nrwl/eslint-plugin-nx": "15.8.3",
