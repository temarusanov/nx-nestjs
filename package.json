{
  "$schema": "https://json.schemastore.org/package",
  "name": "nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --all",
    "container": "nx run-many --target=container --all",
    "prisma:generate": "nx run-many --target=prisma:generate --all",
    "test": "nx test",
    "prepare": "husky install",
    "lint": "npm run nx -- run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix && nx format:write --all"
