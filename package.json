    "@nx-tools/container-metadata": "^4.0.3",
    "@nx-tools/nx-container": "^4.0.3",
    "@types/jest": "29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "eslint": "~8.15.0",
    "husky": "^8.0.1",
    "jest": "29.4.3",
