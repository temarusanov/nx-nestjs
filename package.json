  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^3.0.0",
    "@nestjs/schematics": "^9.2.0",
    "@nx-go/nx-go": "^2.7.0",
