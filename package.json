    "@nestjs/core": "9.4.3",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/testing": "9.4.3",
    "@prisma/client": "^4.15.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "clsx": "^1.2.1",
