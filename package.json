    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/testing": "9.4.3",
    "@prisma/client": "^5.0.0",
    "@tematools/eventloop-frozen-detector": "^0.3.1",
    "@tematools/health-checks": "^1.0.0",
    "@tematools/logger": "^0.1.1",
    "@tematools/nats": "^0.4.0",
