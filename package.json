{
  "name": "nx",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx run-many --target=build --all",
    "container": "nx run-many --target=container --all",
    "prisma:generate": "nx run-many --target=prisma:generate --all",
    "test": "nx test",
    "prepare": "husky install",
    "lint": "npm run tsc:lint && nx workspace-lint && npm run nx -- run-many --target=lint --all",
    "lint:fix": "npm run tsc:lint && nx workspace-lint --fix && nx run-many --target=lint --all --fix && nx format:write --all",
    "tsc:lint": "tsc --noEmit -p tsconfig.base.json"
  },
  "private": true,
  "dependencies": {
    "@actions/exec": "^1.1.1",
    "@apollo/server": "^4.6.0",
    "@graphql-yoga/nestjs": "^1.0.0",
    "@nestjs/apollo": "^11.0.4",
    "@nestjs/common": "9.4.3",
    "@nestjs/core": "9.4.3",
    "@nestjs/graphql": "^11.0.4",
    "@nestjs/platform-express": "9.4.3",
    "@nestjs/testing": "9.4.3",
    "@prisma/client": "^4.15.0",
    "bcrypt": "^5.1.0",
    "clsx": "^1.2.1",
    "env-var": "^7.3.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.9.1",
    "nestjs-custom-injector": "^2.2.3",
    "pg": "^8.8.0",
    "pg-promise": "^10.12.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "9.2.0",
    "@nx-go/nx-go": "^2.7.0",
    "@nx-tools/container-metadata": "^4.0.3",
    "@nx-tools/nx-container": "^4.0.3",
    "@nx/devkit": "16.3.2",
    "@nx/eslint-plugin": "16.3.2",
    "@nx/jest": "16.3.2",
    "@nx/js": "16.3.2",
    "@nx/linter": "16.3.2",
    "@nx/nest": "16.3.2",
    "@nx/node": "16.3.2",
    "@nx/plugin": "16.3.2",
    "@nx/webpack": "16.3.2",
    "@nx/workspace": "16.3.2",
    "@swc-node/register": "^1.6.5",
    "@swc/core": "^1.3.64",
    "@types/jest": "29.4.0",
    "@types/node": "18.7.1",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "eslint": "~8.15.0",
    "husky": "^8.0.1",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-environment-node": "^29.4.1",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.0.3",
    "nx": "16.3.2",
    "nx-cloud": "16.0.5",
    "prettier": "^2.6.2",
    "prisma": "^4.15.0",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix"
  }
}
