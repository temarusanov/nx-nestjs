    "@nx/plugin": "16.5.0",
    "@nx/webpack": "16.5.0",
    "@nx/workspace": "16.5.0",
    "@swc-node/register": "^1.6.6",
    "@swc/core": "^1.3.68",
    "@tematools/schematics": "^0.2.0",
    "@types/jest": "29.5.2",
    "@types/node": "20.4.0",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "eslint": "~8.44.0",
