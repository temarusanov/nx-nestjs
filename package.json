    "husky": "^8.0.3",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "jest-environment-node": "^29.5.0",
    "jsonc-eslint-parser": "^2.3.0",
    "lint-staged": "^13.2.2",
    "nx": "16.3.2",
    "nx-cloud": "16.0.5",
    "prettier": "^2.8.8",
    "prisma": "^4.15.0",
