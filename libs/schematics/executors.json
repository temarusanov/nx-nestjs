{
  "$schema": "http://json-schema.org/schema",
  "executors": {
    "prisma-generate": {
      "implementation": "./src/executors/prisma/generate/executor",
      "schema": "./src/executors/prisma/generate/schema.json",
      "description": "Generate all affected Prisma schemas"
    },
    "prisma-pull": {
      "implementation": "./src/executors/prisma/pull/executor",
      "schema": "./src/executors/prisma/pull/schema.json",
      "description": "Pull all affected Prisma schemas"
    },
    "prisma-migrate": {
      "implementation": "./src/executors/prisma/migrate/executor",
      "schema": "./src/executors/prisma/migrate/schema.json",
      "description": "Run Prisma migrations"
    },
    "graphql-gateway-compose": {
      "implementation": "./src/executors/graphql/gateway-compose/executor",
      "schema": "./src/executors/graphql/gateway-compose/schema.json",
      "description": "Run Apollo Rover supergraph composer"
    }
  }
}
